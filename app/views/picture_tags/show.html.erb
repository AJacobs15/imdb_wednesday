<div class="page-header">
  <h3>Picture Tag #<%= @picture_tag.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Actor</dt>
      <dd><%= @picture_tag.actor_id %></dd>

      <dt>Picture</dt>
      <dd><%= @picture_tag.picture_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/picture_tags" class="btn btn-primary">
        Back
      </a>
      <a href="/picture_tags/<%= @picture_tag.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_picture_tag/<%= @picture_tag.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
